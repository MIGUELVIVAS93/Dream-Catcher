<p id="notice"><%= notice %></p>

<h1>Datos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Documento</th>
      <th>Numdocumento</th>
      <th>Telefono</th>
      <th>Correo</th>
      <th>Pais</th>
      <th>Usuario</th>
      <th>Usuario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @datos.each do |dato| %>
      <tr>
        <td><%= dato.nombre %></td>
        <td><%= dato.documento %></td>
        <td><%= dato.numdocumento %></td>
        <td><%= dato.telefono %></td>
        <td><%= dato.correo %></td>
        <td><%= dato.pais %></td>
        <td><%= dato.usuario %></td>
        <td><%= dato.usuario %></td>
        <td><%= link_to 'Show', dato %></td>
        <td><%= link_to 'Edit', edit_dato_path(dato) %></td>
        <td><%= link_to 'Destroy', dato, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dato', new_dato_path %>
